<div class="col-sm-12 py-sm-3 pl-sm-5 bd-content">
  <div class="row">
    <div class="dropdown-divider"></div>
    <div class="col-sm-12">
      <h3 class="bd-title">
        Todos os seus pedidos
      </h3>
      <div class="row">
        {{#each model as |solicitation|}}
          {{#solicitation-status solicitation=solicitation}}
            {{#if (eq solicitation.status 'Finalizado')}}
              <b>Qual sua avaliação para o serviço?</b>
              {{star-rating rating=solicitation.ratingProfessional wholeOnly=true onClick=(route-action 'avaliation' solicitation)}}
            {{/if}}
          {{/solicitation-status}}
        {{/each}}
      </div>
    </div>
  </div>
</div>